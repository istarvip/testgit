    <declare-styleable name="groupTopTitle">
        <attr name="leftContent" format="string"/>
        <attr name="centerContent" format="string"/>
    </declare-styleable>

    <declare-styleable name="groupItemContent">
        <attr name="leftItemContent" format="string"/>
        <attr name="centerItemContent" format="string"/>
        <attr name="rightItemContent" format="string"/>
    </declare-styleable>

    <declare-styleable name="watchSettingItem">
