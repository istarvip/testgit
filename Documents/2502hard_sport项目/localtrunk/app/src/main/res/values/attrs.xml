        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>


    <declare-styleable name="simpleLabel">
        <attr name="label" format="string"/>
        <attr name="labelValue" format="string"/>
    </declare-styleable>

    <declare-styleable name="topTitle">
        <attr name="back" format="string"/>
        <attr name="lableText" format="string"/>
        <attr name="right_oper_img" format="reference" />
